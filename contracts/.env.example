# MumbleChat Contract Deployment Configuration
# Copy this to .env and fill in your values

# Deployer wallet private key (WITHOUT 0x prefix)
# ⚠️ NEVER commit your actual private key!
DEPLOYER_PRIVATE_KEY=your_private_key_here

# Token distribution wallets
COMMUNITY_POOL=0x...
TEAM_WALLET=0x...
ECOSYSTEM_WALLET=0x...
LIQUIDITY_WALLET=0x...

# Block explorer API key (for contract verification)
RAMASCAN_API_KEY=your_api_key_here
